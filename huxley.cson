#=========================
# Huxley Manifest File
#=========================
# This file is used to specify your project's cluster and what Huxley launches into it.

# The public SSH keys of every person you'd like to have access to the cluster.
public_keys: [
  "ssh-rsa  robb@pandstrike.com (common)",
  "ssh-rsa  david@interlincx.com",
  "ssh-rsa  peterlongnguyen@gmail.com",
  "ssh-dss ",
  "ssh-rsa  real.david.harper@gmail.com",
  "ssh-rsa  lance@pandastrike.com"
]

#=============================
# Cluster Description
#=============================
stack_name: "Vanilla"     # Required
key_pair: "CoreOS-Test"   # Required
channel: "stable"

hostname: "vanilla.pandastrike.com"
private_hosted_zone: "vanilla.cluster"

# **WARNING** - These affect the cost of your cluster.  See AWS Documentation for pricing.
instance_type: "m1.medium"
cluster_size: 3
spot_price: 0.009
#=============================

# Optional Tag Descriptions
tags: [
  {
    Key: "Name"
    Value: "Vanilla"
  }
  {
    Key: "customer"
    Value: "pandastrike"
  }
  {
    Key: "environment"
    Value: "test"
  }
  {
    Key: "project"
    Value: "huxley"
  }
  {
    Key: "role"
    Value: "coreos"
  }
]


#=============================
# Template Descriptions
#=============================
# Template configurations of services that are needed *during* cluster formation.  These are limited
# to services that change properties of the instances themselves.
formation_service_templates:
  format_ephemeral: "default"
  var_lib_docker: "default"

# This section configures services that are deployed *after* cluster formation and the machines are
# ready (most services).
# Services prefixed with "pc_" are templates stored in the panda-cluster library.

# Template values are specified as members of the unit's object.
# Specifiying "null" as the object's value will deliver the servie file "as is".
service_units:
  vanilla: null
  pc_elasticsearch:
    output_filename: "elasticsearch.service"
    after: ["docker.service"]
    container_name: "elasticsearch"
    image_name: "pandastrike/pc_elasticsearch"
    hostname: "elasticsearch.vanilla.cluster"
    port: "2001"
    type: "A"

  pc_kibana:
    output_filename: "kibana.service"
    after: ["docker.service", "elasticsearch.service"]
    container_name: "kibana"
    image_name: "pandastrike/pc_kibana"
    hostname: "kibana.huxley.pandastrike.com"
    port: "80"
    elasticsearch_url: "elasticsearch.vanilla.cluster:2001"
